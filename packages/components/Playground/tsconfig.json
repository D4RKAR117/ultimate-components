{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "../../../tsconfig.base.json",
	"compilerOptions": {
		"jsx": "preserve",
		"jsxImportSource": "@builder.io/mitosis",
		"moduleResolution": "Node",
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"],
			"@components/*": ["src/components/*"]
		}
	},
	"include": ["src/**/*.lite.tsx", "src/**/*.ts"],
	"exclude": ["dist"]
}
